{
    "name": "elephox/apcu-cache",
    "description": "A cache implementation using the PECL APCu extension.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Boss",
            "email": "contact@ricardoboss.de"
        }
    ],
    "require": {
        "php": "^8.1 <8.3",
        "ext-apcu": "*",
        "elephox/cache": "dev-develop"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master"
    },
    "provide": {
        "psr/cache-implementation": "3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Elephox\\Cache\\APCu\\": "src/"
        }
    }
}
